{
  "name": "moss-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "concurrently --names tsc,vite \"tsc -b\" \"vite build\"",
    "watch": "concurrently --names tsc,vite \"tsc -b -w --preserveWatchOutput\" \"vite --clearScreen false\"",
    "lint": "eslint src",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.1.4",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.17",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
